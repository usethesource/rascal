            "projectName": "rascal-lsp",
            "hostName": "localhost",
            "port": 9001
        },
        {
            "type": "java",
            "name": "Attach RascalNotebook",
            "request": "attach",
            "hostName": "localhost",
            "port": 8000,
            "projectName": "rascal-notebook"
        },
