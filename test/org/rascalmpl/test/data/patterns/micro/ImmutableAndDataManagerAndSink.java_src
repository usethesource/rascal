package org.rascalmpl.test.data.patterns.micro;

public class ImmutableAndDataManagerAndSink {
	private double f;
	private int i;

	public ImmutableAndDataManagerAndSink(int i, double f) {
		this.i = i;
		this.f = f;
	}
	
	public double getF() {
		return f;
	}
	
	public int getI() {
		return i;
	}

}
