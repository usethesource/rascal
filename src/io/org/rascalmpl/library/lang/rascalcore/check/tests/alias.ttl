test A1 [[ module A alias INT = int; INT z = 13; ]]

test A2 [[ module A INT z = 13; ]] 
expect { "Invalid variable initialization;" }

test A3 [[ module A alias INT = int; INT[str] z = 13; ]] 
expect { "Expected 0 type parameters for `int`, found 1" }

