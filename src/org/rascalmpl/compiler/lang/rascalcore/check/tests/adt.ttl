test A1 [[  module A
            data D = d1(int n, bool b = false);
            data D = d2(str s, int m = 0);
            data D = d3(bool f, str t = "a");
            
            D X1 = d1(3);       D X2 = d1(3, b=false);
            D Y1 = d2("z");     D Y2 = d2("z", m=1);
            D Z1 = d3(true);    D Z2 = d3(true, t ="z");
]]

