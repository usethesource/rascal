            D Y1 = d2("z");     D Y2 = d2("z", m=1);    D Y3 = d2("z", m=1, N=1, S="z",B=true);
            D Z1 = d3(true);   D Z2 = d3(true, t ="z");D Z3 = d3(true, t ="z", N=1, S="z",B=true);
]]

